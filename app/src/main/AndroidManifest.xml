<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luchobolivar.hospitaleleden">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeCustome">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistroActivity" />
        <activity android:name=".AdministradorActivity" />
        <activity android:name=".PacienteActivity" />
        <activity android:name=".PersonalMedicoActivity" />
        <activity android:name=".Consulta_registroActivity" />
        <activity android:name=".Edicion_CitaActivity" />
        <activity android:name=".Historial_CitasActivity" />
        <activity android:name=".Listado_sedeActivity" />
        <activity android:name=".DireccionActivity"></activity>
    </application>

</manifest>